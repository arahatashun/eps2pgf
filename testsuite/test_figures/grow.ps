%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 95 98 437 343

8 dict begin

/knopregel {
    steel
    gsave
        knop
    grestore
    gsave
        -22.5 rotate
        knop
    grestore
    steel
    gsave
        22.5 rotate
        steel
        knop
    grestore
    -22.5 rotate
    knop
} bind def

/steelregel {
    steel
    steel
} bind def

/knop {
    dup 0 eq {
        currentpoint
        3 3 3 3 6 0 rcurveto
        -3 -3 -3 -3 -6 0 rcurveto
        .2 .2 0  setrgbcolor
        fill moveto
    } {
        dup 1 sub
        knopregel
        pop
    } ifelse
} bind def

/steel {
    dup 0 eq {
        6 0 rlineto
        0 .5 0 setrgbcolor
        currentpoint stroke moveto
    } {
        dup 1 sub
        steelregel
        pop
    } ifelse
} bind def

1 setlinecap
1 setlinejoin
.5 setlinewidth
gsave
    100 100 moveto
    90 rotate
    0 knop pop
    stroke
grestore
gsave
    170 100 moveto
    90 rotate
    1 knop pop
    stroke
grestore
gsave
    240 100 moveto
    90 rotate
    2 knop pop
    stroke
grestore
gsave
    310 100 moveto
    90 rotate
    3 knop pop
    stroke
grestore
gsave
    380 100 moveto
    90 rotate
    4 knop pop
    stroke
grestore

end
showpage

%%EOF
